    }
  },
  {
    "apiVersion": "apps/v1",
    "kind": "Deployment",
    "metadata": {
      "name": "release-name-pgbouncer",
      "labels": {
        "app.kubernetes.io/managed-by": "Helm",
        "helm.sh/chart": "pgbouncer-1.0.8",
        "app.kubernetes.io/name": "release-name-pgbouncer",
        "app.kubernetes.io/instance": "release-name"
      }
    },
    "spec": {
      "strategy": {
        "rollingUpdate": {
          "maxUnavailable": 0
        }
      },
      "replicas": 1,
      "selector": {
        "matchLabels": {
          "app.kubernetes.io/name": "release-name-pgbouncer",
          "app.kubernetes.io/instance": "release-name"
        }
      },
      "template": {
        "metadata": {
          "labels": {
            "app.kubernetes.io/managed-by": "Helm",
            "helm.sh/chart": "pgbouncer-1.0.8",
            "app.kubernetes.io/name": "release-name-pgbouncer",
            "app.kubernetes.io/instance": "release-name"
          },
          "annotations": {
            "checksum/ini": "a11d342c2bb33b6e2f59b6540b522b556bbf4f035429cf6a72671410cfe30eee",
            "checksum/users": "e3b0c44298fc1c149afbf4c8996fb92427ae41e4649b934ca495991b7852b855"
          }
        },
        "spec": {
          "affinity": {
            "podAntiAffinity": {
              "preferredDuringSchedulingIgnoredDuringExecution": [
                {
                  "weight": 1,
                  "podAffinityTerm": {
                    "topologyKey": "kubernetes.io/hostname",
                    "labelSelector": {
                      "matchLabels": {
                        "app.kubernetes.io/name": "release-name-pgbouncer",
                        "app.kubernetes.io/instance": "release-name"
                      }
                    }
                  }
                }
              ]
            }
          },
          "volumes": [
            {
              "name": "secret",
