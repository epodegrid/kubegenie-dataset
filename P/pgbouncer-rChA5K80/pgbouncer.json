            }
          ],
          "containers": [
            {
              "name": "pgbouncer",
              "image": "cradlepoint/pgbouncer:latest",
              "imagePullPolicy": "Always",
              "ports": [
                {
                  "name": "pgbouncer",
                  "containerPort": 5432
                }
              ],
              "readinessProbe": {
                "tcpSocket": {
                  "port": 5432
                }
              },
              "lifecycle": {
                "preStop": {
                  "exec": {
                    "command": [
                      "/bin/sh",
                      "-c",
                      "sleep 5"
                    ]
                  }
                }
              },
              "volumeMounts": [
                {
                  "name": "secret",
                  "subPath": "userlist.txt",
                  "mountPath": "/etc/pgbouncer/userlist.txt",
                  "readOnly": true
                },
                {
                  "name": "secret",
                  "subPath": "pgbouncer.ini",
                  "mountPath": "/etc/pgbouncer/pgbouncer.ini",
                  "readOnly": true
