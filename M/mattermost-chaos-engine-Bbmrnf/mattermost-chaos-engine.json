        "app.kubernetes.io/name": "mattermost-chaos-engine",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/managed-by": "Helm"
      },
      "annotations": {
        "helm.sh/hook": "test"
      }
    },
    "spec": {
      "containers": [
        {
          "name": "wget",
          "image": "busybox",
          "command": [
            "wget"
          ],
          "args": [
            "release-name-mattermost-chaos-engine:3000/bindings"
          ]
        }
      ],
      "restartPolicy": "Never"
    }
  }
