            "app.kubernetes.io/instance": "release-name"
          }
        },
        "spec": {
          "serviceAccountName": "release-name-mattermost-chaos-engine",
          "securityContext": {},
          "containers": [
            {
              "name": "mattermost-chaos-engine",
              "securityContext": {
                "allowPrivilegeEscalation": false,
                "capabilities": {
                  "drop": {
                    "": "NET_RAW"
