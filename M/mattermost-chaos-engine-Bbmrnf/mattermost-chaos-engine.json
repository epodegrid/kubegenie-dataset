  {
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
      "name": "release-name-mattermost-chaos-engine",
      "labels": {
        "helm.sh/chart": "mattermost-chaos-engine-0.2.0",
        "app.kubernetes.io/name": "mattermost-chaos-engine",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/managed-by": "Helm"
      }
    },
    "spec": {
      "type": "ClusterIP",
      "ports": [
        {
          "port": 3000,
          "targetPort": "http",
          "protocol": "TCP",
          "name": "http"
        }
      ],
      "selector": {
        "app.kubernetes.io/name": "mattermost-chaos-engine",
        "app.kubernetes.io/instance": "release-name"
      }
    }
  },
  {
    "apiVersion": "apps/v1",
    "kind": "Deployment",
    "metadata": {
      "name": "release-name-mattermost-chaos-engine",
      "labels": {
        "helm.sh/chart": "mattermost-chaos-engine-0.2.0",
        "app.kubernetes.io/name": "mattermost-chaos-engine",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/managed-by": "Helm"
      }
    },
    "spec": {
      "replicas": 1,
      "selector": {
        "matchLabels": {
          "app.kubernetes.io/name": "mattermost-chaos-engine",
          "app.kubernetes.io/instance": "release-name"
        }
      },
      "template": {
        "metadata": {
