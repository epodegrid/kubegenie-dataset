              ],
              "resources": {
                "seccompProfile": {
                  "type": "RuntimeDefault"
                }
              },
              "env": null,
              "envFrom": [
                {
                  "secretRef": {
                    "name": "release-name-mattermost-chaos-engine"
                  }
                }
              ]
            }
          ]
        }
      }
    }
  },
  {
