[
  {
    "apiVersion": "v1",
    "kind": "ServiceAccount",
    "metadata": {
      "name": "release-name-gtm-server-container-cluster",
      "labels": {
        "helm.sh/chart": "gtm-server-container-cluster-0.1.2",
        "app.kubernetes.io/name": "gtm-server-container-cluster",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "1.16.0",
        "app.kubernetes.io/managed-by": "Helm"
      }
    }
  },
  {
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
      "name": "release-name-gtm-server-container-cluster",
      "labels": {
        "helm.sh/chart": "gtm-server-container-cluster-0.1.2",
        "app.kubernetes.io/name": "gtm-server-container-cluster",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "1.16.0",
        "app.kubernetes.io/managed-by": "Helm"
      }
    },
    "spec": {
      "type": "ClusterIP",
      "ports": [
        {
          "port": 8080,
          "targetPort": "http",
          "protocol": "TCP",
          "name": "http"
        }
      ],
      "selector": {
        "app.kubernetes.io/name": "gtm-server-container-cluster",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/type": "tagging"
      }
    }
  },
  {
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
      "name": "release-name-gtm-server-container-cluster-preview",
      "labels": {
        "helm.sh/chart": "gtm-server-container-cluster-0.1.2",
        "app.kubernetes.io/name": "gtm-server-container-cluster",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "1.16.0",
        "app.kubernetes.io/managed-by": "Helm"
      }
    },
    "spec": {
      "type": "ClusterIP",
      "ports": [
        {
          "port": 8080,
          "targetPort": "http",
          "protocol": "TCP",
          "name": "http"
        }
      ],
      "selector": {
        "app.kubernetes.io/type": "preview"
      }
    }
  },
  {
    "apiVersion": "apps/v1",
    "kind": "Deployment",
    "metadata": {
      "name": "release-name-gtm-server-container-cluster",
      "labels": {
        "helm.sh/chart": "gtm-server-container-cluster-0.1.2",
        "app.kubernetes.io/name": "gtm-server-container-cluster",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "1.16.0",
        "app.kubernetes.io/managed-by": "Helm"
      }
    },
    "spec": {
      "replicas": 1,
      "selector": {
        "matchLabels": {
          "app.kubernetes.io/name": "gtm-server-container-cluster",
          "app.kubernetes.io/instance": "release-name"
        }
      },
      "template": {
        "metadata": {
          "labels": {
            "app.kubernetes.io/name": "gtm-server-container-cluster",
            "app.kubernetes.io/instance": "release-name",
            "app.kubernetes.io/type": "tagging"
          }
        },
        "spec": {
          "serviceAccountName": "release-name-gtm-server-container-cluster",
          "securityContext": {},
          "containers": [
            {
              "name": "gtm-server-container-cluster",
              "securityContext": {
                "allowPrivilegeEscalation": false,
                "capabilities": {
                  "drop": {
                    "": "NET_RAW"
                  }
                },
                "readOnlyRootFilesystem": true,
                "seccompProfile": {
                  "type": "RuntimeDefault"
                }
              },
              "image": "gcr.io/cloud-tagging-10302018/gtm-cloud-image:stable",
              "imagePullPolicy": "IfNotPresent",
              "ports": [
                {
                  "name": "http",
                  "containerPort": 8080,
                  "protocol": "TCP"
                }
              ],
              "livenessProbe": {
                "httpGet": {
                  "path": "/healthz",
                  "port": "http"
                }
              },
              "readinessProbe": {
                "httpGet": {
                  "path": "/healthz",
                  "port": "http"
                }
              },
              "resources": {
                "limits": {
                  "cpu": "100m",
                  "memory": "100Mi"
                },
                "requests": {
                  "cpu": "100m",
                  "memory": "100Mi"
                }
              },
              "env": [
                {
                  "name": "CONTAINER_CONFIG",
                  "value": null
                },
                {
                  "name": "PREVIEW_SERVER_URL",
                  "value": "https://chart-example.local/preview"
                }
              ]
            }
          ]
        }
      }
    }
  },
  {
    "apiVersion": "apps/v1",
    "kind": "Deployment",
    "metadata": {
      "name": "release-name-gtm-server-container-cluster-preview",
      "labels": {
        "helm.sh/chart": "gtm-server-container-cluster-0.1.2",
        "app.kubernetes.io/name": "gtm-server-container-cluster",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "1.16.0",
        "app.kubernetes.io/managed-by": "Helm"
      }
    },
    "spec": {
      "replicas": 1,
      "selector": {
        "matchLabels": {
          "app.kubernetes.io/name": "gtm-server-container-cluster",
          "app.kubernetes.io/instance": "release-name"
        }
      },
      "template": {
        "metadata": {
          "labels": {
            "app.kubernetes.io/name": "gtm-server-container-cluster",
            "app.kubernetes.io/instance": "release-name",
            "app.kubernetes.io/type": "preview"
          }
        },
        "spec": {
          "serviceAccountName": "release-name-gtm-server-container-cluster",
          "securityContext": {},
          "containers": [
            {
              "name": "gtm-server-container-cluster",
              "securityContext": {
                "allowPrivilegeEscalation": false,
                "capabilities": {
                  "drop": {
                    "": "NET_RAW"
                  }
                },
                "readOnlyRootFilesystem": true,
                "seccompProfile": {
                  "type": "RuntimeDefault"
                }
              },
              "image": "gcr.io/cloud-tagging-10302018/gtm-cloud-image:stable",
              "imagePullPolicy": "IfNotPresent",
              "ports": [
                {
                  "name": "http",
                  "containerPort": 8080,
                  "protocol": "TCP"
                }
              ],
              "livenessProbe": {
                "httpGet": {
                  "path": "/healthz",
                  "port": "http"
                }
              },
              "readinessProbe": {
                "httpGet": {
                  "path": "/healthz",
                  "port": "http"
                }
              },
              "resources": {
                "limits": {
                  "cpu": "100m",
                  "memory": "100Mi"
                },
                "requests": {
                  "cpu": "100m",
                  "memory": "100Mi"
                }
              },
              "env": [
                {
                  "name": "CONTAINER_CONFIG",
                  "value": null
                },
                {
                  "name": "RUN_AS_PREVIEW_SERVER",
                  "value": "true"
                }
              ]
            }
          ]
        }
      }
    }
  },
  {
    "apiVersion": "networking.k8s.io/v1",
    "kind": "Ingress",
    "metadata": {
      "name": "release-name-gtm-server-container-cluster",
      "labels": {
        "helm.sh/chart": "gtm-server-container-cluster-0.1.2",
        "app.kubernetes.io/name": "gtm-server-container-cluster",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "1.16.0",
        "app.kubernetes.io/managed-by": "Helm"
      },
      "annotations": {
        "cert-manager.io/acme-challenge-type": "http01",
        "cert-manager.io/cluster-issuer": "letsencrypt-prod"
      }
    },
    "spec": {
      "tls": [
        {
          "hosts": [
            "chart-example.local"
          ],
          "secretName": "release-name-gtm-server-container-cluster-tls-secret"
        }
      ],
      "rules": [
        {
          "host": "chart-example.local",
          "http": {
            "paths": [
              {
                "path": "/",
                "pathType": "ImplementationSpecific",
                "backend": {
                  "service": {
                    "name": "release-name-gtm-server-container-cluster",
                    "port": {
                      "number": 8080
                    }
                  }
                }
              },
              {
                "path": "/preview",
                "pathType": "ImplementationSpecific",
                "backend": {
                  "service": {
                    "name": "release-name-gtm-server-container-cluster-preview",
                    "port": {
                      "number": 8080
                    }
                  }
                }
              }
            ]
          }
        }
      ]
    }
  },
  {
    "apiVersion": "v1",
    "kind": "Pod",
    "metadata": {
      "name": "release-name-gtm-server-container-cluster-test-connection",
      "labels": {
        "helm.sh/chart": "gtm-server-container-cluster-0.1.2",
        "app.kubernetes.io/name": "gtm-server-container-cluster",
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/version": "1.16.0",
        "app.kubernetes.io/managed-by": "Helm"
      },
      "annotations": {
        "helm.sh/hook": "test"
      }
    },
    "spec": {
      "containers": [
        {
          "name": "wget",
          "image": "busybox",
          "command": [
            "wget"
          ],
          "args": [
            "release-name-gtm-server-container-cluster:8080"
          ]
        }
      ],
      "restartPolicy": "Never"
    }
  }
]