[
  {
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
      "name": "release-name-influxdb-exporter",
      "labels": {
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/managed-by": "Helm",
        "app.kubernetes.io/name": "influxdb-exporter",
        "app.kubernetes.io/version": "v0.9.0",
        "helm.sh/chart": "influxdb-exporter-1.0.4"
      },
      "annotations": null
    },
    "spec": {
      "type": "ClusterIP",
      "ports": [
        {
          "port": 9122,
          "targetPort": 9122,
          "protocol": "TCP",
          "name": "http"
        }
      ],
      "selector": {
        "app.kubernetes.io/name": "influxdb-exporter",
        "app.kubernetes.io/instance": "release-name"
      }
    }
  },
  {
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
      "name": "release-name-influxdb-exporter-udp",
      "labels": {
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/managed-by": "Helm",
        "app.kubernetes.io/name": "influxdb-exporter",
        "app.kubernetes.io/version": "v0.9.0",
        "helm.sh/chart": "influxdb-exporter-1.0.4"
      },
      "annotations": null
    },
    "spec": {
      "type": "LoadBalancer",
      "ports": [
        {
          "port": 9123,
          "targetPort": 9123,
          "protocol": "UDP",
          "name": "udp"
        }
      ],
      "selector": {
        "app.kubernetes.io/name": "influxdb-exporter",
        "app.kubernetes.io/instance": "release-name"
      }
    }
  },
  {
    "apiVersion": "apps/v1",
    "kind": "Deployment",
    "metadata": {
      "name": "release-name-influxdb-exporter",
      "labels": {
        "app.kubernetes.io/instance": "release-name",
        "app.kubernetes.io/managed-by": "Helm",
        "app.kubernetes.io/name": "influxdb-exporter",
        "app.kubernetes.io/version": "v0.9.0",
        "helm.sh/chart": "influxdb-exporter-1.0.4"
      }
    },
    "spec": {
      "revisionHistoryLimit": 3,
      "replicas": 1,
      "strategy": {
        "type": "Recreate"
      },
      "selector": {
        "matchLabels": {
          "app.kubernetes.io/name": "influxdb-exporter",
          "app.kubernetes.io/instance": "release-name"
        }
      },
      "template": {
        "metadata": {
          "labels": {
            "app.kubernetes.io/name": "influxdb-exporter",
            "app.kubernetes.io/instance": "release-name"
          }
        },
        "spec": {
          "serviceAccountName": "default",
          "automountServiceAccountToken": true,
          "dnsPolicy": "ClusterFirst",
          "enableServiceLinks": true,
          "containers": [
            {
              "name": "release-name-influxdb-exporter",
              "image": "prom/influxdb-exporter:v0.9.0",
              "imagePullPolicy": "IfNotPresent",
              "args": [
                "--udp.bind-address=0.0.0.0:9123"
              ],
              "env": [
                {
                  "name": "TZ",
                  "value": "UTC"
                }
              ],
              "ports": [
                {
                  "name": "http",
                  "containerPort": 9122,
                  "protocol": "TCP"
                },
                {
                  "name": "udp",
                  "containerPort": 9123,
                  "protocol": "UDP"
                }
              ],
              "livenessProbe": {
                "tcpSocket": {
                  "port": 9122
                },
                "initialDelaySeconds": 0,
                "failureThreshold": 3,
                "timeoutSeconds": 1,
                "periodSeconds": 10
              },
              "readinessProbe": {
                "tcpSocket": {
                  "port": 9122
                },
                "initialDelaySeconds": 0,
                "failureThreshold": 3,
                "timeoutSeconds": 1,
                "periodSeconds": 10
              },
              "startupProbe": {
                "tcpSocket": {
                  "port": 9122
                },
                "initialDelaySeconds": 0,
                "failureThreshold": 30,
                "timeoutSeconds": 1,
                "periodSeconds": 5
              }
            }
          ]
        }
      }
    }
  }
]