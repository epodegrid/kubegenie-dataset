                },
                {
                  "name": "PRIVATE_KEY",
                  "valueFrom": {
                    "secretKeyRef": {
                      "name": "release-name-forms-catalogue",
                      "key": "app_private_key"
                    }
                  }
                },
                {
                  "name": "PUBLIC_KEY",
                  "valueFrom": {
                    "secretKeyRef": {
                      "name": "release-name-forms-catalogue",
                      "key": "app_public_key"
                    }
                  }
                },
                {
                  "name": "APP_URL",
                  "valueFrom": {
                    "configMapKeyRef": {
                      "name": "release-name-forms-catalogue",
                      "key": "app-url"
                    }
                  }
                },
                {
