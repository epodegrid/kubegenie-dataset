      "name": "release-name",
      "labels": {
        "app.kubernetes.io/name": "release-name",
        "app.kubernetes.io/part-of": "release-name",
        "helm.sh/chart": "forms-catalogue-0.1.0",
        "app.kubernetes.io/instance": "release-name",
