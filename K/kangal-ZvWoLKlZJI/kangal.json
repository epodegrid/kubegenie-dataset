  {
    "apiVersion": "apps/v1",
    "kind": "Deployment",
    "metadata": {
      "name": "release-name-kangal-controller",
      "labels": {
        "app": "kangal-controller",
        "chart": "kangal-2.2.2",
        "release": "release-name",
        "heritage": "Helm",
        "version": ""
      }
    },
    "spec": {
      "replicas": 1,
      "selector": {
        "matchLabels": {
          "app": "kangal-controller",
          "release": "release-name"
        }
      },
      "template": {
        "metadata": {
          "labels": {
            "app": "kangal-controller",
            "release": "release-name",
            "environment": "dev",
            "version": ""
          }
        },
        "spec": {
          "dnsConfig": {
            "options": [
              {
                "name": "ndots",
                "value": "1"
              }
            ]
          },
          "containers": [
            {
              "name": "kangal-controller",
              "image": "hellofresh/kangal:latest",
              "imagePullPolicy": "Always",
              "args": [
                "controller"
              ],
              "env": [
                {
                  "name": "POD_NAME",
                  "valueFrom": {
                    "fieldRef": {
                      "fieldPath": "metadata.name"
                    }
                  }
                },
                {
                  "name": "KANGAL_PROXY_URL",
                  "value": "https://kangal-proxy.example.com"
                }
              ],
              "envFrom": [
                {
                  "configMapRef": {
                    "name": "release-name-kangal"
                  }
                },
                {
                  "secretRef": {
                    "name": "release-name-kangal"
                  }
                }
              ],
              "ports": [
                {
                  "name": "http",
                  "containerPort": 8080,
                  "protocol": "TCP"
                }
              ],
              "resources": {
                "seccompProfile": {
                  "type": "RuntimeDefault"
                }
              }
            }
          ]
        }
      }
    }
  },
  {
    "apiVersion": "apps/v1",
