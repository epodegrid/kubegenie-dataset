[
  {
    "apiVersion": "apps/v1",
    "kind": "Deployment",
    "metadata": {
      "name": "sqs-prometheus-exporter",
      "namespace": "fOxIfaMEtSWF",
      "labels": {
        "app": "sqs-prometheus-exporter",
        "chart": "sqs-prometheus-exporter-1.1.2",
        "release": "release-name",
        "heritage": "Helm"
      }
    },
    "spec": {
      "replicas": 1,
      "selector": {
        "matchLabels": {
          "app": "sqs-prometheus-exporter",
          "release": "release-name"
        }
      },
      "template": {
        "metadata": {
          "labels": {
            "app": "sqs-prometheus-exporter",
            "release": "release-name"
          },
          "annotations": {
            "prometheus.io/scrape": "true"
          }
        },
        "spec": {
          "containers": [
            {
              "name": "sqs-prometheus-exporter",
              "image": "jmriebold/sqs-prometheus-exporter:1.0.3",
              "imagePullPolicy": "IfNotPresent",
              "ports": [
                {
                  "name": "traffic",
                  "containerPort": 8080,
                  "protocol": "TCP"
                }
              ],
              "resources": null,
              "env": [
                {
                  "name": "SQS_QUEUE_URLS",
                  "value": ""
                },
                {
                  "name": "SQS_MONITOR_INTERVAL_SECONDS",
                  "value": "30"
                },
                {
                  "name": "AWS_REGION",
                  "value": "us-west-2"
                }
              ],
              "livenessProbe": {
                "httpGet": {
                  "path": "/healthz",
                  "port": "traffic"
                },
                "initialDelaySeconds": 3,
                "timeoutSeconds": 5,
                "periodSeconds": 30
              }
            }
          ],
          "restartPolicy": "Always",
          "nodeSelector": null,
          "affinity": null,
          "tolerations": null
        }
      }
    }
  }
]