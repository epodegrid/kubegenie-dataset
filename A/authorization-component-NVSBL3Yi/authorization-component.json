                  "valueFrom": {
                    "configMapKeyRef": {
                      "name": "release-name-authorization-component",
                      "key": "app-version"
                    }
                  }
                },
                {
                  "name": "APP_DEBUG",
                  "valueFrom": {
                    "configMapKeyRef": {
                      "name": "release-name-authorization-component",
                      "key": "debug"
                    }
                  }
                },
                {
                  "name": "APP_CACHE",
                  "valueFrom": {
                    "configMapKeyRef": {
                      "name": "release-name-authorization-component",
                      "key": "cache"
                    }
                  }
                },
                {
